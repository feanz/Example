@model Web.ViewModels.BookmarkViewModel
@{ ViewBag.Title = "Details"; }

<h2>Details</h2>
<div class="Title"><a href="@Model.Url">@Model.Title</a></div>
<div class="Url"><a href="@Model.Url">@Model.Url</a></div>
<div class="DateCreated">Started on @Html.DisplayFor(model => model.DateStarted)</div>
<div class="Description">@Model.Description</div>
<div class="Type">@Model.BookmarkTypeTypeName</div>
<div class="Tags">@Model.Tags</div>

<ul class="Actions">    
    <li>@Html.ActionLink("Back to List", "Index")</li>
</ul>
